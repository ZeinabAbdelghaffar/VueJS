{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref } from \"vue\";\nimport AdminTable from '../Slots/AdminTable.vue';\nimport UsersTable from '../Slots/UsersTable.vue';\nimport UserForm from \"../UserForm.vue\";\nimport { users } from \"./usersData.js\";\nimport { admins } from \"./adminsData.js\";\nexport default {\n  components: {\n    UserForm,\n    UsersTable,\n    AdminTable\n  },\n  setup() {\n    let id = 11;\n    let flag = ref(\"UsersTable\");\n    let deleteUser = (id, role) => {\n      if (role == \"user\") {\n        users.value = users.value.filter(u => u.id !== id);\n      } else if (role == \"admin\") {\n        users.value = users.value.filter(u => u.id !== id);\n      }\n    };\n    let addUser = (name, age, role) => {\n      const newUser = {\n        id: id++,\n        name,\n        age,\n        role\n      };\n      if (role == \"admin\") {\n        admins.value.push(newUser);\n      } else if (role == \"user\") {\n        users.value.push(newUser);\n      }\n    };\n    let toggle = name => {\n      flag.value = name;\n    };\n    return {\n      id,\n      flag,\n      users,\n      admins,\n      deleteUser,\n      addUser,\n      toggle\n    };\n  }\n};","map":{"version":3,"names":["ref","AdminTable","UsersTable","UserForm","users","admins","components","setup","id","flag","deleteUser","role","value","filter","u","addUser","name","age","newUser","push","toggle"],"sources":["C:\\Users\\Lenovo-\\Desktop\\zeinab\\VueJS\\day2\\src\\Components\\Pages\\HomePage.vue"],"sourcesContent":["<template>\n  <user-form @addUser=\"addUser\"></user-form>\n  <div class=\"mx-auto max-w-screen-xl px-4 sm:px-6 lg:px-8\">\n    <div class=\"hidden sm:block\">\n      <div class=\"flex gap-6\" aria-label=\"Tabs\">\n        <button\n          @click=\"toggle('UsersTable')\"\n          :class=\" flag == 'UsersTable'?'bg-gray-300 text-gray-700': ''\"\n          class=\"shrink-0 rounded-lg p-5 text-sm font-medium text-gray-500 hover:bg-gray-300 hover:text-gray-700\"\n        >\n          User\n        </button>\n  \n        <button\n          @click=\"toggle('AdminTable')\"\n          :class=\" flag == 'AdminTable'?'bg-gray-300 text-gray-700': ''\"\n          class=\"shrink-0 rounded-lg p-5 text-sm font-medium text-gray-500 hover:bg-gray-300 hover:text-gray-700\"\n        >\n          Admin\n        </button>\n      </div>\n    </div>\n    \n    <div class=\"mb-24\">\n      <component :is=\"flag\" @deleteUser=\"deleteUser\" :users=\"users\" :admins=\"admins\"/>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { ref } from \"vue\";\nimport AdminTable from '../Slots/AdminTable.vue';\nimport UsersTable from '../Slots/UsersTable.vue';\nimport UserForm from \"../UserForm.vue\";\nimport { users } from \"./usersData.js\"; \nimport { admins } from \"./adminsData.js\"; \n\n  export default {\n    components: {\n      UserForm,\n      UsersTable,\n      AdminTable\n    },\n    setup() {\n      let id = 11;\n      let flag = ref(\"UsersTable\");\n\n      let deleteUser = (id, role) => {\n        if(role == \"user\") {\n          users.value = users.value.filter(u => u.id !== id);\n        } else if(role == \"admin\") {\n          users.value = users.value.filter(u => u.id !== id);\n        }\n      }\n      let addUser = (name, age, role) => {\n        const newUser = {\n            id: id++,\n            name,\n            age,\n            role,\n          }\n        if(role == \"admin\") {\n          admins.value.push(newUser)\n        } else if (role == \"user\") {\n          users.value.push(newUser)\n        }\n      }\n      let toggle = (name) => {\n        flag.value = name\n      }\n\n      return {\n        id, flag, users, admins, deleteUser, addUser, toggle\n      }\n  }\n}\n</script>"],"mappings":";AA8BA,SAASA,GAAE,QAAS,KAAK;AACzB,OAAOC,UAAS,MAAO,yBAAyB;AAChD,OAAOC,UAAS,MAAO,yBAAyB;AAChD,OAAOC,QAAO,MAAO,iBAAiB;AACtC,SAASC,KAAI,QAAS,gBAAgB;AACtC,SAASC,MAAK,QAAS,iBAAiB;AAEtC,eAAe;EACbC,UAAU,EAAE