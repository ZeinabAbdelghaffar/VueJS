{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  data() {\n    return {\n      name: '',\n      age: '',\n      role: 'user',\n      users: [],\n      filteredUsers: [],\n      filter: ''\n    };\n  },\n  methods: {\n    addUser() {\n      const newUser = {\n        name: this.name,\n        age: this.age,\n        role: this.role\n      };\n      this.users.push(newUser);\n      this.resetForm();\n      this.applyFilter();\n    },\n    deleteUser(index) {\n      this.users.splice(index, 1);\n      this.applyFilter();\n    },\n    filterRole(role) {\n      this.filter = role;\n      this.applyFilter();\n    },\n    applyFilter() {\n      if (this.filter) {\n        this.filteredUsers = this.users.filter(user => user.role === this.filter);\n      } else {\n        this.filteredUsers = this.users;\n      }\n    },\n    resetForm() {\n      this.name = '';\n      this.age = '';\n      this.role = 'user';\n    }\n  }\n};","map":{"version":3,"names":["data","name","age","role","users","filteredUsers","filter","methods","addUser","newUser","push","resetForm","applyFilter","deleteUser","index","splice","filterRole","user"],"sources":["C:\\Users\\Lenovo-\\Desktop\\zeinab\\VueJS\\day1\\src\\components\\UserForm.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <h2 class=\"form-title\">Add User Form</h2>\n    <form @submit.prevent=\"addUser\">\n      <div class=\"form-group\">\n        <label for=\"name\">Name</label>\n        <input type=\"text\" id=\"name\" v-model=\"name\" required />\n      </div>\n\n      <div class=\"form-group\">\n        <label for=\"age\">Age</label>\n        <input type=\"number\" id=\"age\" v-model=\"age\" required />\n      </div>\n\n      <div class=\"form-group\">\n        <label for=\"role\">Role</label>\n        <select id=\"role\" v-model=\"role\" required>\n          <option value=\"user\">User</option>\n          <option value=\"admin\">Admin</option>\n        </select>\n      </div>\n\n      <button type=\"submit\">Add</button>\n    </form>\n\n    <div class=\"buttons\">\n      <button @click=\"filterRole('admin')\">List Admins</button>\n      <button @click=\"filterRole('user')\">List Users</button>\n    </div>\n\n    <ul class=\"user-list\">\n      <li v-for=\"(person, index) in filteredUsers\" :key=\"index\">\n        {{ person.name }} ({{ person.age }}) - {{ person.role }}\n        <button @click=\"deleteUser(index)\">Delete</button>\n      </li>\n    </ul>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      name: '',\n      age: '',\n      role: 'user',\n      users: [],\n      filteredUsers: [],\n      filter: '',\n    };\n  },\n  methods: {\n    addUser() {\n      const newUser = {\n        name: this.name,\n        age: this.age,\n        role: this.role,\n      };\n      this.users.push(newUser);\n      this.resetForm();\n      this.applyFilter();\n    },\n    deleteUser(index) {\n      this.users.splice(index, 1);\n      this.applyFilter();\n    },\n    filterRole(role) {\n      this.filter = role;\n      this.applyFilter();\n    },\n    applyFilter() {\n      if (this.filter) {\n        this.filteredUsers = this.users.filter(user => user.role === this.filter);\n      } else {\n        this.filteredUsers = this.users;\n      }\n    },\n    resetForm() {\n      this.name = '';\n      this.age = '';\n      this.role = 'user';\n    },\n  },\n};\n</script>\n\n<style scoped>\n.container {\n  max-width: 600px;\n  margin: 0 auto;\n  padding: 20px;\n  background-color: #f9f9f9;\n  border-radius: 8px;\n  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\n}\n\n.form-title {\n  text-align: center;\n  margin-bottom: 20px;\n  font-size: 24px;\n  color: #333;\n}\n\nform {\n  margin-top: 50px;\n}\n\n.form-group {\n  margin-bottom: 15px;\n}\n\n.form-group label {\n  display: block;\n  margin-bottom: 5px;\n  font-weight: 600;\n}\n\n.form-group input,\n.form-group select {\n  width: 100%;\n  padding: 10px;\n  border: 1px solid #ccc;\n  border-radius: 4px;\n  box-sizing: border-box;\n  transition: border-color 0.3s;\n}\n\n.form-group input:focus,\n.form-group select:focus {\n  border-color: #41b883;\n  outline: none;\n}\n\nbutton {\n  padding: 10px 20px;\n  margin-right: 10px;\n  margin-bottom: 10px;\n  background-color: #41b883;\n  color: #fff;\n  border: none;\n  border-radius: 4px;\n  cursor: pointer;\n  transition: background-color 0.3s;\n}\n\nbutton:hover {\n  background-color: #35495e;\n}\n\n.buttons {\n  display: flex;\n  justify-content: space-between;\n  margin-top: 20px;\n}\n\n.user-list {\n  margin-top: 20px;\n  list-style: none;\n  padding: 0;\n}\n\n.user-list li {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 10px;\n  border-bottom: 1px solid #eee;\n}\n\n.user-list button {\n  background-color: #e74c3c;\n  padding: 5px 10px;\n  border: none;\n  border-radius: 4px;\n  color: #fff;\n  cursor: pointer;\n  transition: background-color 0.3s;\n}\n\n.user-list button:hover {\n  background-color: #c0392b;\n}\n</style>\n"],"mappings":";AAwCA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,EAAE;MACRC,GAAG,EAAE,EAAE;MACPC,IAAI,EAAE,MAAM;MACZC,KAAK,EAAE,EAAE;MACTC,aAAa,EAAE,EAAE;MACjBC,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,OAAOA,CAAA,EAAG;MACR,MAAMC,OAAM,GAAI;QACdR,IAAI,EAAE,IAAI,CAACA,IAAI;QACfC,GAAG,EAAE,IAAI,CAACA,GAAG;QACbC,IAAI,EAAE,IAAI,CAACA;MACb,CAAC;MACD,IAAI,CAACC,KAAK,CAACM,IAAI,CAACD,OAAO,CAAC;MACxB,IAAI,CAACE,SAAS,CAAC,CAAC;MAChB,IAAI,CAACC,WAAW,CAAC,CAAC;IACpB,CAAC;IACDC,UAAUA,CAACC,KAAK,EAAE;MAChB,IAAI,CAACV,KAAK,CAACW,MAAM,CAACD,KAAK,EAAE,CAAC,CAAC;MAC3B,IAAI,CAACF,WAAW,CAAC,CAAC;IACpB,CAAC;IACDI,UAAUA,CAACb,IAAI,EAAE;MACf,IAAI,CAACG,MAAK,GAAIH,IAAI;MAClB,IAAI,CAACS,WAAW,CAAC,CAAC;IACpB,CAAC;IACDA,WAAWA,CAAA,EAAG;MACZ,IAAI,IAAI,CAACN,MAAM,EAAE;QACf,IAAI,CAACD,aAAY,GAAI,IAAI,CAACD,KAAK,CAACE,MAAM,CAACW,IAAG,IAAKA,IAAI,CAACd,IAAG,KAAM,IAAI,CAACG,MAAM,CAAC;MAC3E,OAAO;QACL,IAAI,CAACD,aAAY,GAAI,IAAI,CAACD,KAAK;MACjC