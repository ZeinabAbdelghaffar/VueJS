group select {\n  width: 100%;\n  padding: 8px;\n  box-sizing: border-box;\n}\n\nbutton {\n  margin-right: 10px;\n  padding: 10px 20px;\n  background-color: #41b883;\n  color: #fff;\n  border: none;\n  cursor: pointer;\n}\n\nbutton:hover {\n  background-color: #35495e;\n}\n\n.user-list {\n  margin-top: 20px;\n  list-style: none;\n  padding: 0;\n}\n\n.user-list li {\n  margin-bottom: 10px;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n}\n\n.user-list button {\n  background-color: #e74c3c;\n  padding: 5px 10px;\n  border: none;\n  color: #fff;\n  cursor: pointer;\n}\n\n.user-list button:hover {\n  background-color: #c0392b;\n}\n</style>"],"mappings":"AAuCA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,EAAE;MACRC,GAAG,EAAE,CAAC;MACNC,IAAI,EAAE;IACR;EACF,CAAC;EACDC,KAAK,EAAE,CAAC,SAAS,CAAC;EAClBC,KAAK,EAAE,CAAC,OAAO,EAAE,QAAQ;AAC3B","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}